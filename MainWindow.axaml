<Window xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		x:Class="CardGameClient.MainWindow"
		Title="Card Game" FontFamily="Noto Sans">
	<StackPanel Spacing="5" VerticalAlignment="Center">
		<Button Click="ToDuelClick"
				HorizontalAlignment="Center"
				VerticalAlignment="Center">
			Duel
		</Button>
		<Button Click="ToDeckEditClick"
				HorizontalAlignment="Center"
				VerticalAlignment="Center">
			Deck Edit
		</Button>
		<Button Click="ToReplaysClick"
				HorizontalAlignment="Center"
				VerticalAlignment="Center">
			Replays
		</Button>
		<Button Click="ToSettingsClick"
				HorizontalAlignment="Center"
				VerticalAlignment="Center">
			Settings
		</Button>
	</StackPanel>
</Window>
